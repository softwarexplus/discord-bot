{
    "name": "discord-bot",
    "version": "1.0.0",
    "main": "index.js",
    "type": "commonjs",
    "author": "SoftwareX Plus",
    "license": "MIT",
    "scripts": {
        "fmt": "prettier --config=.prettierrc --write .",
